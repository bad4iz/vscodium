// Place your key bindings in this file to override the defaultsauto[]
[
   {
      "key": "ctrl+k",
      "command": "workbench.action.navigateUp"
   },
   {
      "key": "ctrl+h",
      "command": "workbench.action.navigateLeft"
   },
   {
      "key": "ctrl+j",
      "command": "workbench.action.navigateDown"
   },
   {
      "key": "ctrl+l",
      "command": "workbench.action.navigateRight"  
   },
   {
      "key": "space",
      "command": "whichkey.show", 
      "when": "editorTextFocus && neovim.init && neovim.mode !== insert",
   },
   {
      "key": "space c",
      "command": "workbench.action.closeActiveEditor",
      "when": "editorTextFocus && neovim.init && neovim.mode !== insert",
   },
   {
      "key": "ctrl+w",
      "command": "-workbench.action.closeActiveEditor"
   },
   {
      "key": "shift+l",
      "command": "workbench.action.nextEditor"
   },
   {
      "key": "shift+h",
      "command": "workbench.action.previousEditor"
   },
   {
      "key": "space e",
      "command": "workbench.view.explorer",
      "when": "viewContainer.workbench.view.explorer.enabled && editorTextFocus && neovim.init && neovim.mode !== insert",
   },
   {
      "key": "shift+alt+k",
      "command": "workbench.action.moveEditorToAboveGroup"
   },
   {
      "key": "shift+alt+j",
      "command": "workbench.action.moveEditorToBelowGroup"
   },
   {
      "key": "shift+alt+h",
      "command": "workbench.action.moveEditorToLeftGroup"
   },
   {
      "key": "shift+alt+l",
      "command": "workbench.action.moveEditorToRightGroup"
   },
    {
      "key": "q",
      "command": "workbench.action.toggleSidebarVisibility",
      "when": "viewContainer.workbench.view.explorer.enabled && neovim.mode != 'insert'"
   }
]

