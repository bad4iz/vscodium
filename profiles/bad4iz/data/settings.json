{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "genieai.enableConversationHistory": true,
  "git.autofetch": true,
  "syncSettings.crons": {
    "download": "0 9 * * *", // at 9AM, every day
    "review": "0 * * * *", // every hour
    "upload": "0 17 * * *" // at 5PM, every day
  },
  "terminal.integrated.env.windows": {},
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "j"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "after": ["shift+tab"],
      "command": "workbench.action.quickOpenNavigatePrevious",
      "when": "inQuickOpen"
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditor"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    },
    // {
    //   "after": ["d", "d"],
    //   "before": ["<leader>", "d"]
    // },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    // {
    //   "before": ["<leader>" , "s"],
    //   "commands": ["leap.find"]
    // }
    {
      "after": ["leader", "leader", "s"],
      "before": ["s"]
    },
    {
      "after": ["leader", "leader", "F"],
      "before": ["F"]
    },
    {
      "before": ["leader", "g", "g"],
      "command": "chatgpt-vscode.freeText"
    },
    {
      "before": ["leader", "l", "a"],
      "command": ["editor.action.quickFix"]
    }
  ],
  "vim.scroll": 10,
  "vim.sneak": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": "#5f0000",
  "vim.statusBarColors.normal": "#4444db",
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    }
  ],
  "whichkey.bindingOverrides": [
    {
      "command": "workbench.action.closeActiveEditor",
      "keys": "c",
      "name": "Close tab",
      "type": "command"
    }
  ],
  "whichkey.delay": 0,
  "whichkey.sortOrder": "alphabetically",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564",
    "statusBar.background": "#4444db",
    "statusBar.noFolderBackground": "#4444db",
    "statusBar.debuggingBackground": "#4444db"
  },
  "workbench.colorTheme": "Tokyo Night nv",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.tree.indent": 16,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  }
}
